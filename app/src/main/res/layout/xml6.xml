<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    >


    <TextView
        android:id="@+id/textView6"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"

        android:textSize="30dp"
        android:gravity="center"
        android:textColor="@color/purple_700"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/subject6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="SUBJECT"
        android:textColor="@color/purple_700"
        android:textSize="30dp"
        app:layout_constraintStart_toStartOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        tools:layout_editor_absoluteX="64dp"
        android:layout_marginLeft="40dp"
        tools:layout_editor_absoluteY="59dp" />
    <TextView
        android:id="@+id/cred6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CREDITS"
        android:textColor="@color/purple_700"
        android:textSize="30dp"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        app:layout_constraintLeft_toRightOf="@+id/subject6"
        android:layout_marginLeft="70dp"
        />

    <EditText
        android:id="@+id/text6"
        android:layout_width="192dp"
        android:layout_height="40dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="4dp"
        android:drawablePadding="10dp"
        android:ems="10"

        android:inputType="number"
        android:paddingLeft="20dp"
        android:paddingTop="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="10dp"


        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/subject6" />

    <EditText
        android:id="@+id/spin16"
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="8dp"
        android:inputType="number"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/text6"

        app:layout_constraintTop_toBottomOf="@+id/cred6"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/editText6"
        android:layout_width="0dp"
        android:layout_height="43dp"
        android:layout_marginTop="20dp"
        android:drawablePadding="10dp"
        android:paddingLeft="20dp"
        android:paddingTop="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="10dp"
        android:textSize="20dp"
        android:inputType="number"
        app:layout_constraintEnd_toEndOf="@+id/text6"
        app:layout_constraintStart_toStartOf="@+id/text6"
        app:layout_constraintTop_toBottomOf="@id/text6"
        />

    <EditText
        android:id="@+id/spin26"
        android:layout_width="80dp"
        android:layout_height="43dp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="15dp"
        android:inputType="number"


        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/editText6"
        app:layout_constraintTop_toBottomOf="@+id/spin16"
        app:layout_constraintVertical_bias="0.001" />

    <EditText
        android:id="@+id/editText16"
        android:layout_width="0dp"
        android:layout_height="43dp"
        android:layout_marginTop="20dp"
        android:drawablePadding="10dp"
        android:paddingLeft="20dp"
        android:paddingTop="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="10dp"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/text6"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/text6"
        app:layout_constraintTop_toBottomOf="@id/editText6"
        android:inputType="number"/>

    <EditText
        android:id="@+id/spin36"
        android:layout_width="80dp"
        android:layout_height="43dp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="15dp"

        android:inputType="number"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/editText16"
        app:layout_constraintTop_toBottomOf="@+id/spin26"
        app:layout_constraintVertical_bias="0.014" />

    <EditText
        android:id="@+id/editText26"
        android:layout_width="0dp"
        android:layout_height="43dp"
        android:layout_marginTop="20dp"
        android:drawablePadding="10dp"
        android:paddingLeft="20dp"
        android:paddingTop="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="10dp"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/text6"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/text6"
        app:layout_constraintTop_toBottomOf="@id/editText16"
        android:inputType="number"/>

    <EditText
        android:id="@+id/spin46"
        android:layout_width="80dp"
        android:layout_height="43dp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="20dp"

        android:inputType="number"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/editText26"
        app:layout_constraintTop_toBottomOf="@+id/spin36"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/editText36"
        android:layout_width="0dp"
        android:layout_height="43dp"
        android:layout_marginTop="20dp"
        android:drawablePadding="10dp"
        android:paddingLeft="20dp"
        android:paddingTop="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="10dp"
        android:textSize="20dp"
        android:inputType="number"
        app:layout_constraintEnd_toEndOf="@+id/text6"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/text6"
        app:layout_constraintTop_toBottomOf="@id/editText26" />

    <EditText
        android:id="@+id/spin56"
        android:layout_width="80dp"
        android:layout_height="43dp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="20dp"


        android:inputType="number"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/editText36"
        app:layout_constraintTop_toBottomOf="@+id/spin46"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/editText46"
        android:layout_width="0dp"
        android:layout_height="43dp"
        android:layout_marginTop="20dp"
        android:drawablePadding="10dp"
        android:paddingLeft="20dp"
        android:paddingTop="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="10dp"
        android:textSize="20dp"
        android:inputType="number"
        app:layout_constraintEnd_toEndOf="@+id/text6"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/text6"
        app:layout_constraintTop_toBottomOf="@id/editText36" />

    <EditText
        android:id="@+id/spin66"
        android:layout_width="80dp"
        android:layout_height="43dp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="20dp"

        android:inputType="number"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/editText46"
        app:layout_constraintTop_toBottomOf="@+id/spin56"
        app:layout_constraintVertical_bias="0.0" />




    <Button
        android:id="@+id/b16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="12dp"
        android:clickable="true"
        android:text="Calculate"
        app:layout_constraintEnd_toStartOf="@+id/textView116"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editText46"
        android:inputType="number"/>

    <TextView
        android:id="@+id/textView116"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"



        android:textSize="20dp"
        app:layout_constraintStart_toEndOf="@+id/b16"
        app:layout_constraintTop_toBottomOf="@id/editText46" />

</androidx.constraintlayout.widget.ConstraintLayout>

