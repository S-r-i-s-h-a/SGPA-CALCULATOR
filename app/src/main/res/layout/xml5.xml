<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    >


    <TextView
        android:id="@+id/textView65"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"

        android:textSize="30dp"
        android:gravity="center"
        android:textColor="@color/purple_700"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/subject65"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="SUBJECT"
        android:textColor="@color/purple_700"
        android:textSize="30dp"
        app:layout_constraintStart_toStartOf="@+id/textView65"
        app:layout_constraintTop_toBottomOf="@+id/textView65"
        tools:layout_editor_absoluteX="64dp"
        android:layout_marginLeft="40dp"
        tools:layout_editor_absoluteY="59dp" />
    <TextView
        android:id="@+id/cred65"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CREDITS"
        android:textColor="@color/purple_700"
        android:textSize="30dp"
        app:layout_constraintTop_toBottomOf="@+id/textView65"
        app:layout_constraintLeft_toRightOf="@+id/subject65"
        android:layout_marginLeft="70dp"
        />

    <EditText
        android:id="@+id/text65"
        android:layout_width="192dp"
        android:layout_height="40dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="4dp"
        android:drawablePadding="10dp"
        android:ems="10"

        android:inputType="number"
        android:paddingLeft="20dp"
        android:paddingTop="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="10dp"


        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/subject65" />

    <EditText
        android:id="@+id/spin165"
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="8dp"
        android:inputType="number"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/text65"

        app:layout_constraintTop_toBottomOf="@+id/cred65"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/editText65"
        android:layout_width="0dp"
        android:layout_height="43dp"
        android:layout_marginTop="20dp"
        android:drawablePadding="10dp"
        android:paddingLeft="20dp"
        android:paddingTop="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="10dp"
        android:textSize="20dp"
        android:inputType="number"
        app:layout_constraintEnd_toEndOf="@+id/text65"
        app:layout_constraintStart_toStartOf="@+id/text65"
        app:layout_constraintTop_toBottomOf="@id/text65"
        />

    <EditText
        android:id="@+id/spin265"
        android:layout_width="80dp"
        android:layout_height="43dp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="15dp"
        android:inputType="number"


        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/editText65"
        app:layout_constraintTop_toBottomOf="@+id/spin165"
        app:layout_constraintVertical_bias="0.001" />

    <EditText
        android:id="@+id/editText165"
        android:layout_width="0dp"
        android:layout_height="43dp"
        android:layout_marginTop="20dp"
        android:drawablePadding="10dp"
        android:paddingLeft="20dp"
        android:paddingTop="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="10dp"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/text65"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/text65"
        app:layout_constraintTop_toBottomOf="@id/editText65"
        android:inputType="number"/>

    <EditText
        android:id="@+id/spin365"
        android:layout_width="80dp"
        android:layout_height="43dp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="15dp"

        android:inputType="number"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/editText165"
        app:layout_constraintTop_toBottomOf="@+id/spin265"
        app:layout_constraintVertical_bias="0.014" />

    <EditText
        android:id="@+id/editText265"
        android:layout_width="0dp"
        android:layout_height="43dp"
        android:layout_marginTop="20dp"
        android:drawablePadding="10dp"
        android:paddingLeft="20dp"
        android:paddingTop="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="10dp"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/text65"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/text65"
        app:layout_constraintTop_toBottomOf="@id/editText165"
        android:inputType="number"/>

    <EditText
        android:id="@+id/spin465"
        android:layout_width="80dp"
        android:layout_height="43dp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="20dp"

        android:inputType="number"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/editText265"
        app:layout_constraintTop_toBottomOf="@+id/spin365"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/editText365"
        android:layout_width="0dp"
        android:layout_height="43dp"
        android:layout_marginTop="20dp"
        android:drawablePadding="10dp"
        android:paddingLeft="20dp"
        android:paddingTop="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="10dp"
        android:textSize="20dp"
        android:inputType="number"
        app:layout_constraintEnd_toEndOf="@+id/text65"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/text65"
        app:layout_constraintTop_toBottomOf="@id/editText265" />

    <EditText
        android:id="@+id/spin565"
        android:layout_width="80dp"
        android:layout_height="43dp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="20dp"


        android:inputType="number"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/editText365"
        app:layout_constraintTop_toBottomOf="@+id/spin465"
        app:layout_constraintVertical_bias="0.0" />


    <Button
        android:id="@+id/b165"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:clickable="true"
        android:inputType="number"
        android:text="Calculate"
        app:layout_constraintTop_toBottomOf="@+id/editText365"
        app:layout_constraintEnd_toStartOf="@+id/textView116"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="458dp" />

    <TextView
        android:id="@+id/textView116"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"


        app:layout_constraintTop_toBottomOf="@+id/editText365"
        android:textSize="20dp"
        app:layout_constraintStart_toEndOf="@+id/b165"
        tools:layout_editor_absoluteY="461dp" />

</androidx.constraintlayout.widget.ConstraintLayout>

